{
  "name": "@repo/utils",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "tsc -b --watch",
    "build": "tsc -b -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "main": "./dist/entry.js",
  "types": "./dist/entry.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./request": {
      "types": "./dist/request/index.d.ts",
      "import": "./dist/request/index.js",
      "require": "./dist/request/index.js"
    },
    "./response": {
      "types": "./dist/response/index.d.ts",
      "import": "./dist/response/index.js",
      "require": "./dist/response/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js",
      "require": "./dist/db/index.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  }
}
